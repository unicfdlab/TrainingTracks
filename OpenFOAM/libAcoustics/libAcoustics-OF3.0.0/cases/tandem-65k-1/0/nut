FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           volScalarField;
    object          nut;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }

    outlet
    {
        type            zeroGradient;
    }

    cyl-1
    {
        type            nutkWallFunction;
        value		uniform 0;
    }
    
    cyl-2
    {
        type            nutkWallFunction;
        value		uniform 0;
    }
    
    free
    {
	type		zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }

}


