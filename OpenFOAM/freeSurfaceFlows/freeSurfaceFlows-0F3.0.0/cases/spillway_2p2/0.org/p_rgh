FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inletAir
    {
        type            fixedFluxPressure;
//        type            buoyantPressure;
        value           uniform 0;
    }
    
    inletWater
    {
        type            fixedFluxPressure;
//        type            buoyantPressure;
        value           uniform 0;
    }

    outlet
    {
        type            buoyantPressure;
        value           uniform 0;
    }

    atmosphere
    {
        type            totalPressure;
        p0              uniform 0;
        U               U;
        phi             phi;
        rho             rho;
        psi             none;
        gamma           1;
        value           uniform 0;
    }
    
    bottomWall
    {
        type            buoyantPressure;
        value           uniform 0;
    }
    
    dam_Mesh
    {
        type            buoyantPressure;
        value           uniform 0;
    }

    front
    {
        type            empty;
    }
    
    back
    {
        type            empty;
    }
    
    defaultFaces
    {
        type            empty;
    }
    
}

// ************************************************************************* //
